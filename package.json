{
  "name": "app",
  "private": true,
  "devDependencies": {
    "esbuild": "^0.25.4"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --minify --sourcemap --format=esm --outdir=app/assets/builds --public-path=/assets",
    "build:css": "sass app/javascript/stylesheets/application.scss:app/assets/builds/application.css --no-source-map --load-path=node_modules"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.13",
    "@popperjs/core": "^2.11.8",
    "@rails/actioncable": "^8.0.200",
    "bootstrap": "^5.3.6",
    "font-awesome": "^4.7.0",
    "jquery": "^3.7.1",
    "sass": "^1.88.0",
    "sortablejs": "^1.15.6"
  }
}
